{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@hooks/*": ["src/hooks/*"],
      "@redux/*": ["src/redux/*"],
      "@navigation/*": ["src/navigation/*"],
      "@modules/*": ["src/modules/*"],
      "@assets/*": ["src/assets/*"],
      "@constants/*": ["src/constants/*"],
      "@functions/*": ["src/functions/*"],
      "@types/*": ["src/@types/*"]
    }
  },
  "exclude": ["node_modules"]
}
